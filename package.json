{
  "name": "@gr4vy/cli",
  "version": "0.2.0",
  "description": "The Gr4vy CLI",
  "author": "Gr4vy, Inc <code@gr4vy.com>",
  "bin": {
    "gr4vy": "./bin/run"
  },
  "homepage": "https://github.com/gr4vy/gr4vy-cli",
  "license": "MIT",
  "main": "dist/index.js",
  "repository": "gr4vy/gr4vy-cli",
  "files": [
    "/bin",
    "/dist",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "dependencies": {
    "@gr4vy/sdk": "^1.0.3",
    "@oclif/core": "^4.4.0",
    "@oclif/plugin-autocomplete": "^3.2.31",
    "@oclif/plugin-help": "^6.2.29",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "@types/request": "^2.48.12",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "eslint": "^9.29.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jest": "^29.0.1",
    "eslint-plugin-prettier": "^5.5.1",
    "oclif": "^4.20.1",
    "prettier": "^3.6.2",
    "shx": "^0.4.0",
    "ts-node": "^10.9.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3"
  },
  "oclif": {
    "bin": "gr4vy",
    "dirname": "gr4vy",
    "commands": "./dist/commands",
    "plugins": [
      "@oclif/plugin-help",
      "@oclif/plugin-autocomplete"
    ],
    "macos": {
      "identifier": "com.gr4vy.cli"
    }
  },
  "scripts": {
    "build": "shx rm -rf dist && tsc -b",
    "lint": "eslint . --ext .ts --config .eslintrc.js",
    "postpack": "shx rm -f oclif.manifest.json",
    "posttest": "yarn lint",
    "prepack": "yarn build && oclif manifest && oclif readme",
    "test": "echo \"No tests\"",
    "version": "oclif readme && git add README.md"
  },
  "engines": {
    "node": ">=12.0.0"
  },
  "bugs": "https://github.com/gr4vy/gr4vy-cli/issues",
  "keywords": [
    "oclif"
  ],
  "types": "dist/index.d.ts"
}
